{
	"name": "nepalidateconvert",
	"version": "3.0.2",
	"description": "A javascript package to convert nepali date BS (Bikram Sambat) to AD and vice-versa, calcuate age from nepali date (BS). Fixed Smdkl package till 2083",
	"repository": "Yunitula/EngtoNepDate",
	"main": "dist/nepalidateconvert.umd.js",
	"exports": {
		".": {
			"import": "./dist/nepalidateconvert.es.js",
			"require": "./dist/nepalidateconvert.umd.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"prepare": "npm run build",
		"preview": "vite preview",
		"test": "jest"
	},
	"devDependencies": {
		"@types/jest": "^29.5.1",
		"jest": "^29.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.3.2"
	},
	"files": [
		"dist",
		"types",
		"README.md",
		"package.json"
	],
	"publishConfig": {
		"access": "public"
	},
	"types": "types/index.d.ts",
	"author": "Yunish Tuladhar",
	"license": "MIT",
	"keywords": [
		"nepali age calculator",
		"BS age",
		"nepali-date-converter",
		"nepali-date",
		"english-to-nepali-date",
		"AD-BS converter",
		"BS-AD converter",
		"ad-to-bs",
		"nepali to english date",
		"english to nepali date"
	]
}
